{
  "name": "react-node-app",
  "version": "1.0.0",
  "description": "A Simple API",
  "author": "Pooja Gurung",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --watch src --exec babel-node src",
    "migrate": "yarn knex migrate:latest --knexfile src/knexfile.js",
    "rollback": "yarn knex migrate:rollback --knexfile src/knexfile.js",
    "make:migration": "yarn knex migrate:make --knexfile src/knexfile.js",
    "seed": "yarn knex seed:run --knexfile src/knexfile.js"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "camelize": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-jwt": "^6.0.0",
    "http-status-codes": "^2.1.3",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.5",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "pg": "^8.3.3",
    "rimraf": "^3.0.2",
    "snakeize": "^0.1.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5"
  }
}
